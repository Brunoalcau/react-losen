{
  "name": "react-losen",
  "version": "1.0.3",
  "description": "A super customisable Wizard for React and React Native",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/otovo/react-losen.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "pack build",
    "eslint": "eslint . --cache",
    "docz:dev": "docz dev",
    "docz:build": "cp README.md ./pages/_index.mdx && docz build",
    "pack:publish": "pack publish"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-umd", {"name": "reactLosen"}
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@pika/pack": "^0.3.2",
    "@pika/plugin-build-umd": "^0.3.11",
    "@pika/plugin-build-web": "^0.3.11",
    "@pika/plugin-standard-pkg": "^0.3.11",
    "babel-eslint": "^10.0.1",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-otovo": "https://github.com/otovo/eslint-config-otovo.git",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-flowtype": "^3.4.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.93.0",
    "prettier": "^1.16.4",
    "tachyons": "^4.11.1"
  },
  "dependencies": {
    "loglevel": "^1.6.1",
    "prop-types": "^15.7.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  }
}
